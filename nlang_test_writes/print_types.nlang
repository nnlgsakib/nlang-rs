// Test printing different data types
def main() {
    // Test integer printing
    store num = 42;
    println("Integer value:");
    println(num);
    
    // Test float printing
    store pi = 3.14159;
    println("Float value:");
    println(pi);
    
    // Test boolean printing
    store is_true = true;
    store is_false = false;
    println("Boolean values:");
    println(is_true);
    println(is_false);
    
    // Test string printing
    store message = "Hello from NLang!";
    println("String value:");
    println(message);
    
    // Test expressions
    println("Expression results:");
    println(10 + 5);
    println(20.5 - 5.5);
    println(true && false);
}