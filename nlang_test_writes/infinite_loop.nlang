// Loop test program
def main() {
    store counter = 0;
    println("Starting loop...");
    println("Initial counter value:");
    println(counter);
    
    while (counter < 5) {
        println("Entering loop iteration");
        counter = counter + 1;
        println("Loop iteration: ");
        println(counter);
    }
    
    println("Loop finished!");
    println("Final counter value:");
    println(counter);
}